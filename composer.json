{
    "name": "collabmed/inventory",
    "description": "The Collabmed Platform",
    "keywords": ["inventory", "collabmed", "samuel", "dervis"],
    "license": "MIT",
    "type": "project",
    "require": {
        "php": ">=5.6.4",
        "laravel/framework": "5.3.*",
        "nwidart/laravel-modules": "^0.11.2",
        "asgardcms/adminlte-theme": "^1.15",
        "composer/installers": "~1.0",
        "laravelcollective/html": "5.3.*",
        "laracasts/flash": "^2.0",
        "floatingpoint/stylist": "^0.4.0",
        "maatwebsite/laravel-sidebar": "^2.1",
        "cartalyst/sentinel": "2.0.*",
        "asgardcms/flatly-theme": "^1.2",
        "maddhatter/laravel-fullcalendar": "^1.2",
        "jenssegers/date": "^3.2"
    },
    "require-dev": {
        "fzaninotto/faker": "~1.4",
        "mockery/mockery": "0.9.*",
        "phpunit/phpunit": "~5.0",
        "symfony/css-selector": "3.1.*",
        "symfony/dom-crawler": "3.1.*",
        "barryvdh/laravel-ide-helper": "^2.2",
        "doctrine/dbal": "^2.5",
        "barryvdh/laravel-debugbar": "^2.2",
        "graham-campbell/exceptions": "^9.1",
        "filp/whoops": "^2.1"
    },
    "autoload": {
        "classmap": [
            "database"
        ],
        "psr-4": {
            "Dervis\\": "app/",
            "Ignite\\": "Modules/"
        }
    },
    "autoload-dev": {
        "classmap": [
            "tests/TestCase.php"
        ]
    },
    "scripts": {
        "post-root-package-install": [
            "php -r \"file_exists('.env') || copy('.env.example', '.env');\""
        ],
        "post-create-project-cmd": [
            "php artisan key:generate"
        ],
        "post-install-cmd": [
            "Illuminate\\Foundation\\ComposerScripts::postInstall",
            "php artisan optimize"
        ],
        "post-update-cmd": [
            "Illuminate\\Foundation\\ComposerScripts::postUpdate",
            "php artisan optimize"
        ]
    },
    "config": {
        "preferred-install": "dist"
    }
}
