{
    "name": "collabmed/platform",
    "description": "The Collabmed Platform",
    "keywords": ["inventory", "collabmed", "samuel", "dervis", "healthcare"],
    "license": "proprietary",
    "homepage": "http://www.collabmed.com",
    "authors": [
        {
            "name": "Samuel Odhiambo Okoth",
            "email": "sodhiambo@collabmed.com",
            "role": "developer"
        }
    ],
    "type": "project",
    "require": {
        "php": ">=5.6.4",
        "laravel/framework": "5.3.*",
        "cartalyst/sentinel": "2.0.*",
        "nwidart/laravel-modules": "^1.13",
        "collabmed/flatly-theme": "dev-master",
        "collabmed/adminlte-theme": "dev-master",
        "collabmed/core-module": "dev-master",
        "collabmed/users-module": "dev-master",
        "collabmed/settings-module": "dev-master"
    },
    "require-dev": {
        "fzaninotto/faker": "~1.4",
        "mockery/mockery": "0.9.*",
        "phpunit/phpunit": "~5.0",
        "symfony/css-selector": "3.1.*",
        "symfony/dom-crawler": "3.1.*",
        "barryvdh/laravel-ide-helper": "^2.2",
        "doctrine/dbal": "^2.5",
        "barryvdh/laravel-debugbar": "^2.2",
        "graham-campbell/exceptions": "^9.1",
        "filp/whoops": "^2.1",
        "orchestra/testbench": "~3.1",
        "league/flysystem-aws-s3-v3": "~1.0"
    },
    "autoload": {
        "classmap": [
            "database"
        ],
        "psr-4": {
            "Dervis\\": "app/",
            "Ignite\\": "Modules/"
        }
    },
    "autoload-dev": {
        "classmap": [
            "tests/TestCase.php"
        ]
    },
    "scripts": {
        "post-root-package-install": [
            "php -r \"file_exists('.env') || copy('.env.example', '.env');\""
        ],
        "post-create-project-cmd": [
            "php artisan key:generate"
        ],
        "post-install-cmd": [
            "Illuminate\\Foundation\\ComposerScripts::postInstall",
            "php artisan stylist:publish",
            "php artisan module:publish",
            "php artisan optimize"
        ],
        "post-update-cmd": [
            "Illuminate\\Foundation\\ComposerScripts::postUpdate",
            "php artisan stylist:publish",
            "php artisan module:publish",
            "php artisan optimize"
        ]
    },
    "config": {
        "preferred-install": "dist",
        "secure-http": false
    },
    "repositories": [
        {
            "type": "composer",
            "url": "http://sapi.collabmed.net/repo/private/"
        }
    ]
}
